<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.3/p5.js"></script>
    
    <script>
    var x;
    var y;
    var z;  
    var q;
    var t;
    var k;
    var speed;
    var score;
        function setup() {
            createCanvas(500,400);
        //variables set to value needed
            x = 250;
            y = 0;
            z = random(500)
            q = random(100)
            t = random(600)
            k = random(200)
            speedZ = .50
            speedQ = 0.75
            speedT = 1
            speedK = 0.55
           
        }

        function draw() {
            background(0);
                  rectMode(CENTER)
            
            //falling circles
            ellipse(z+random(0),speedZ*y++,50,50)
            ellipse(q+random(2-3),speedQ*y++,50,50)
            ellipse(t+random(14-15),speedT*y++,50,50)
            ellipse(k+random(20-21),speedK*y++,50,50)
            
            rect(x,400,100,100)//box
        
        }
        
        function keyPressed(){
            if (keyCode === RIGHT_ARROW){
                x = x+50
                //box move to right
            }
            if (keyCode === LEFT_ARROW){
                x = x-50
                //box move to left
            }
        }
  
  </script>
</head>

<body>
    <div id="code" class="container-fluid">
        <script src="https://gist-it.appspot.com/github/lesliel2935/p5js/blob/master/conditional-project.html?footer=0"></script>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script>
        $(window).bind("load", function() {
            $('#code').before($('#defaultCanvas0').remove());
        });
    </script>
</body>


</html>
